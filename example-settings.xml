<?xml version="1.0"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
        http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<servers>
		<server>
			<id>mulesoft-ee-release</id>
			<username>muleNexusUser</username>
			<password>muleNexusPass</password>
		</server>
		<server>
			<id>mulesoft-snapshots</id>
			<username>muleNexusUser</username>
			<password>muleNexusPass</password>
		</server>
		<server>
			<id>mulesoft-private</id>
			<username>muleNexusUser</username>
			<password>muleNexusPass</password>
		</server>
		<server>
			<id>anypoint-platform</id>
			<username>${u}</username>
			<password>${p}</password>
		</server>
	</servers>

	<activeProfiles>
		<activeProfile>global-settings</activeProfile>
		<activeProfile>standard-repositories</activeProfile>
	</activeProfiles>

	<profiles>
		<profile>
			<id>global-settings</id>
			<properties>
				<deploy.businessGroup>My Business Org\My Business Sub-Org</deploy.businessGroup>
				<instance.id>base</instance.id>
				<key.id>base</key.id>
			</properties>
		</profile>
		
		<profile>
			<id>no-environment</id>
			<activation>
				<property>
					<name>env</name>
					<value></value>
				</property>
			</activation>
			<properties>
				<deploy.app.suffix>local</deploy.app.suffix>
				<deploy.environment>LOCAL</deploy.environment>
				<deploy.target></deploy.target>
				<deploy.targetType></deploy.targetType>
			</properties>
		</profile>
		
		<profile>
			<id>dev-01</id>
			<activation>
				<property>
					<name>env</name>
					<value>dev-01</value>
				</property>
			</activation>
			<properties>
				<deploy.app.suffix>mule3</deploy.app.suffix>
				<deploy.environment>DEV</deploy.environment>
				<deploy.target>dev-01</deploy.target>
				<deploy.targetType>server</deploy.targetType>
			</properties>
		</profile>
		
		<profile>
			<id>dev-mule4</id>
			<activation>
				<property>
					<name>env</name>
					<value>dev-mule4</value>
				</property>
			</activation>
			<properties>
				<deploy.app.suffix>mule4</deploy.app.suffix>
				<deploy.environment>DEV</deploy.environment>
				<deploy.target>dev-mule4</deploy.target>
				<deploy.targetType>server</deploy.targetType>
			</properties>
		</profile>
		
		<profile>
			<id>test1</id>
			<activation>
				<property>
					<name>env</name>
					<value>test1</value>
				</property>
			</activation>
			<properties>
				<deploy.app.suffix>test</deploy.app.suffix>
				<deploy.environment>TEST</deploy.environment>
				<deploy.target>test1</deploy.target>
				<deploy.targetType>cluster</deploy.targetType>
			</properties>
		</profile>
		
		<profile>
			<id>test-mule4</id>
			<activation>
				<property>
					<name>env</name>
					<value>test-mule4</value>
				</property>
			</activation>
			<properties>
				<deploy.app.suffix>mule4</deploy.app.suffix>
				<deploy.environment>TEST</deploy.environment>
				<deploy.target>test-mule4</deploy.target>
				<deploy.targetType>cluster</deploy.targetType>
			</properties>
		</profile>
		
		<profile>
			<id>prod1</id>
			<activation>
				<property>
					<name>env</name>
					<value>prod1</value>
				</property>
			</activation>
			<properties>
				<deploy.app.suffix>prod</deploy.app.suffix>
				<deploy.environment>PROD</deploy.environment>
				<deploy.target>prod1</deploy.target>
				<deploy.targetType>cluster</deploy.targetType>
			</properties>
		</profile>
		
		<profile>
			<id>prod-mule4</id>
			<activation>
				<property>
					<name>env</name>
					<value>prod-mule4</value>
				</property>
			</activation>
			<properties>
				<deploy.app.suffix>prod</deploy.app.suffix>
				<deploy.environment>PROD</deploy.environment>
				<deploy.target>prod-mule4</deploy.target>
				<deploy.targetType>cluster</deploy.targetType>
			</properties>
		</profile>

		<profile>
			<id>standard-repositories</id>
			<repositories>
				<repository>
					<id>Central</id>
					<name>Central</name>
					<url>http://repo1.maven.org/maven2/</url>
					<layout>default</layout>
				</repository>

				<repository>
					<id>mulesoft-releases</id>
					<name>MuleSoft Releases Repository</name>
					<url>http://repository.mulesoft.org/releases/</url>
					<layout>default</layout>
				</repository>

				<repository>
					<id>mulesoft-ee-release</id>
					<url>https://repository.mulesoft.org/nexus-ee/content/repositories/releases-ee/</url>
					<layout>default</layout>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</repository>
			</repositories>

			<pluginRepositories>
				<pluginRepository>
					<id>mule-public</id>
					<layout>default</layout>
					<url>https://repository.mulesoft.org/nexus/content/repositories/releases</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>mulesoft-release</id>
					<name>mulesoft release repository</name>
					<layout>default</layout>
					<url>https://repository.mulesoft.org/nexus/content/groups/public</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
				</pluginRepository>
				<pluginRepository>
					<id>mulesoft-ee-release</id>
					<url>https://repository.mulesoft.org/nexus-ee/content/repositories/releases-ee/</url>
					<layout>default</layout>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>
		</profile>
	</profiles>

	<pluginGroups>
		<pluginGroup>org.mule.tools</pluginGroup>
	</pluginGroups>

</settings>
